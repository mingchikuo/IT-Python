    "B = [4, 5, 6]\n",
    "\n",
    "y=[x for x in [A,B]]\n",
    "print(y)    # y==> [[1, 2, 3], [4, 5, 6]]\n",
    "\n",
    "z=[ row[col] for row in y for col in range(len(y[0])) ]\n",
    "print(z)    #[1, 2, 3, 4, 5, 6]"
   ]
